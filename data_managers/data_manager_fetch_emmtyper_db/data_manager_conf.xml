<data_managers>
    <data_manager tool_file="data_manager/emmtyper_db_fetcher.xml" id="emmtyper_db_fetcher" version="0.1+galaxy0">
        <data_table name="emmtyper_db">
            <output>
                <column name="value" />
                <column name="name" />
                <column name="version"/>
                <column name="path" output_ref="outdir">
                    <move type="directory" relativize_symlinks="True">
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">emmtyper_db/${value}</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/emmtyper_db/${value}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
            </output>
        </data_table>
    </data_manager>
</data_managers>